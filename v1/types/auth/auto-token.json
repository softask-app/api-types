  "$id": "https://softask-app.github.io/api-types/v1/types/auth/auto-token.json",
  "title": "Auto-Auth Token",
  "description": "Server issued token authorizing a specific device to use auto-auth.",
  "type": "string",
  "minLength": 64,
  "maxLength": 64,
  "pattern": "^[a-fA-F0-9]{64}$",
  "examples": [
    "87a8999b6a7b43f6b02ebb6db45013b056b022a6b06c4638a7bfcf894d3a9d73"
